<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="#3064f9">

    <item>

        <shape android:shape="rectangle">
            <stroke android:width="2dp"
                android:color="#3064f9"/>
            <solid
                android:color="#FFF"/>
            <corners
                android:radius="50dp"/>

        </shape>
    </item>
    <item android:bottom="2dp" android:top="2dp">
        <shape android:shape="oval">
            <stroke android:width="1dp"
                android:color="@color/blue1"/>

        </shape>
    </item>
</ripple>